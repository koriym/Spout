{
    "name": "mackstar/spout",
    "description":"A BEAR.Sunday based CMS",
    "keywords":[
    	"PHP",
    	"CMS"
    ],
    "authors":[
        {
            "name": "Richard McIntyre"
        }
    ],
    "require":{
        "bear/package":"dev-develop",
        "danielstjules/stringy": "1.*",
        "doctrine/migrations": "1.0.*",
        "rhumsaa/uuid":"2.4",
        "robmorgan/phinx": "*",
        "zendframework/zend-validator": "2.1.*"
    },
    "require-dev":{
        "bear/dev-package":"dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload":{
        "psr-4":{
            "Mackstar\\Spout\\": "src/"
        }
    },
    "extra": {
        "installer-paths": {
            "apps/{$name}/": ["type:bearsunday-app"]
        }
    }
}
