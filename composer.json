{
    "name": "mackstar/spout",
    "description":"A BEAR.Sunday based CMS",
    "license": "MIT",
    "keywords":[
        "PHP",
        "CMS"
    ],
    "authors":[
        {
            "name": "Richard McIntyre"
        }
    ],
    "require":{
        "bear/package":"~0.1@dev",
        "danielstjules/stringy": "~1.0",
        "rhumsaa/uuid":"~2.4",
        "robmorgan/phinx": "~0.3",
        "imagine/imagine": "~0.5",
        "zendframework/zend-validator": "~2.0",
        "aptoma/twig-markdown": "~0.2",
        "michelf/php-markdown": "~1.3"
    },
    "require-dev":{
        "bear/dev-package":"~0.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload":{
        "psr-4":{
            "Mackstar\\Spout\\": "src/"
        }
    },
    "bin": ["bin/spout"],
    "extra": {
      "branch-alias": {
        "dev-master": "0.1.x-dev"
      }
    }
}
